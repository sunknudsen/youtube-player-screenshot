{
  "name": "youtube-player-screenshot",
  "version": "0.0.11",
  "description": "Capture a YouTube player screenshot as a PNG file and, optionally, generate markdown code for it.",
  "engines": {
    "node": ">=10.17.0"
  },
  "bin": {
    "youtube-player-screenshot": "./bin/youtube-player-screenshot.js"
  },
  "main": "build/index.js",
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "chalk": "^4.1.0",
    "clipboardy": "^2.3.0",
    "commander": "^6.2.1",
    "puppeteer": "^5.5.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "code": "tsc -w",
    "build": "rm -fr build/*; tsc",
    "ncu": "ncu -u"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "youtube",
    "player",
    "screenshot",
    "png"
  ],
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/puppeteer": "^5.4.2",
    "npm-check-updates": "^10.2.5",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunknudsen/youtube-player-screenshot.git"
  },
  "bugs": {
    "url": "https://github.com/sunknudsen/youtube-player-screenshot/issues"
  },
  "homepage": "https://github.com/sunknudsen/youtube-player-screenshot#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
